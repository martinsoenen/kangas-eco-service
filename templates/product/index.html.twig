{% extends 'base.html.twig' %}{% block title %}Magasin - {{ parent() }}{% endblock %}{% block body %}<div class="products">    {% for produit in produits %}        {{ produit.nomProduit }} - {{ produit.prixUnitaireHT }}â‚¬ - {{ produit.tauxTVA }}% - {{ produit.presentation }} - {{ produit.descriptionDetaillee }} - {{ produit.SousCategorieProduit }} - {{ produit.UtilisateurAdmin }}        <br><br>    {% endfor %}</div>{% for categ in categories %}    <div class="dropdown">        <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">            {{ categ.nom }}        </button>        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">            {% for souscateg in categ.sousCategorieProduits %}                <a class="dropdown-item" href="#">{{ souscateg.nom }}</a>            {% endfor %}        </div>    </div>    <br>{% endfor %}{% endblock %}